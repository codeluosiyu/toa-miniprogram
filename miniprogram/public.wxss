.vhcenter {
  height: 100%;
  align-items: center;
  justify-content: center;
  display: flex;
}

.nowrapELl {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.pageBg {
  background-color: #f0f0f0;
  min-height: 100vh;
}

.myMask {
  position: fixed;
  left: 0;
  top: 0;
  height: 100vh;
  width: 100vw;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 100;
}

.pupop-content {
  position: relative;
}

.close-popup {
  position: absolute;
  right: 0;
  top: 0;
  width: 70rpx;
  height: 70rpx;
  z-index: 999;
}

.sellout-bg {
  background-color: rgba(0, 0, 0, 0.6) !important;
}
